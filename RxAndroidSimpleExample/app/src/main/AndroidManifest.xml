<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.zachbartholomew.rxandroidsimpleexample">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".SampleRxActivity1">
        </activity>
        <activity android:name=".SampleRxActivity2">
        </activity>
        <activity android:name=".SampleRxActivity3">
        </activity>
        <activity android:name=".SampleRxActivity4">
        </activity>
    </application>

</manifest>